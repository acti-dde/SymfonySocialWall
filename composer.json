        "social",
        "api",
        "facebook",
        "google",
        "twitter",
        "flick",
        "instagram",
        "youtube"
    ],
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Guillaume Manen",
            "email": "g.manen@c2is.fr"
        }
